    "@sapphire/plugin-logger": "^3.0.5",
    "@sapphire/plugin-subcommands": "^4.0.2",
    "@sapphire/ratelimits": "^2.4.6",
    "@sapphire/shapeshift": "^3.9.2",
    "@sapphire/time-utilities": "^1.7.10",

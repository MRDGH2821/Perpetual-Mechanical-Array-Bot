    "dotenv": "^16.3.1",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.0.2",
    "prettier-config-mrdgh2821": "^3.0.0",
    "prettier-plugin-packagejson": "^2.4.5",
    "tsc-watch": "^6.0.4",

{
  "extends": ["eslint-config-airbnb-ts-prettier", ".eslintrc-json.json"],
  "parserOptions": {
    "project": ["tsconfig.json", "tsconfig.eslint.json"]
  },
  "ignorePatterns": ["./.pnp.*"],
  "rules": {
    "no-console": "off",
    "class-methods-use-this": [
      "error",
      {
        "exceptMethods": [
          "chatInputRun",
          "run",
          "printBanner",
          "registerApplicationCommands",
          "autocompleteRun"
        ]
      }
    ]
  }
}

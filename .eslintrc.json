{
  "extends": ["eslint-config-ruinguard", "eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "arrow-parens": ["error", "always"],
    "array-bracket-newline": ["error", { "multiline": true }],
    "function-paren-newline": ["error", { "minItems": 4 }]
  }
}
